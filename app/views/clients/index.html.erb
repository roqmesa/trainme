<h1> feed </h1>

<table>
  <thead>
    <tr>
      <th> Title </th>
      <th> Body </th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |post| %>
      <tr>
        <td> <%= client.title %> </td>
        <td> <%= client.body %> </td>
        <td> <%= link_to 'Show', post %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Create a new client post', new_client_path %>